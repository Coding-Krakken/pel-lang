// PEL-100 Benchmark: Marketplace — Rental (equipment/homes)
// Utilization, booking fees, and seasonality

model marketplace_rental {
  param listings: Count<Listing> = 2_000
  param avg_nightly_rate: Currency<USD> = $120
  param avg_occupancy_pct: Fraction = 0.40
  param avg_nights_per_month: Fraction = 10
  param platform_take: Fraction = 0.18

  var monthly_bookings: Count<Booking> = listings * avg_occupancy_pct * avg_nights_per_month
  var gmv: Currency<USD> = monthly_bookings * avg_nightly_rate
  var platform_revenue: Currency<USD> = gmv * platform_take

  constraint liquidity: monthly_bookings >= monthly_bookings {
    severity: warning,
    message: "No bookings — check occupancy assumptions"
  }
}
